<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<system.web>
		<compilation xdt:Transform="RemoveAttributes(debug)" />
		<customErrors mode="RemoteOnly" xdt:Transform="SetAttributes(mode)"/>
	</system.web>
	<elmah>
		<security allowRemoteAccess="false" xdt:Transform="SetAttributes(allowRemoteAccess)" />
	</elmah>
	<appSettings>
		<add key="Google.ClientId" value="" xdt:Transform="Replace" xdt:Locator="Match(key)" />
		<add key="Google.ClientSecret" value="" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
	</appSettings>
</configuration>